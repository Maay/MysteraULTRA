{
  "manifest_version": 2,

  "name": "MysteraULTRA",
  "description": "Adds functionality to Mystera Legacy",
  "version": "0",
  "background":{
    "scripts":["background.js"]
  },
  "externally_connectable":{
    "matches":[
      "*://www.mysteralegacy.com/*"
    ]
  },
  "web_accessible_resources":[
    "runner.js",
    "socketHack.js",
    "breaksound.wav",
    "healthsound.wav"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
    "permissions": [
    "*://www.mysteralegacy.com/*",
    "storage",
    "https://docs.google.com/forms/d/1FAIpQLSdnO_M7j4txY1bxCa3kX-JrAI9Y2wVv60ufAK9qcD0MVe-qtw/formResponse"
  ],
  "content_scripts":[
    {
      "matches":["*://www.mysteralegacy.com/play/full.php","*://www.mysteralegacy.com/play-mystera-legacy/"],
      "js":["contentscript.js"],
      "run_at":"document_start"
    }
  ]
}